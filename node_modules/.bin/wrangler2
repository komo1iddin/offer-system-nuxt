#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules/wrangler/bin/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules/wrangler/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules/wrangler/bin/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules/wrangler/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules/wrangler/bin/wrangler.js" "$@"
else
  exec node  "$basedir/../.pnpm/wrangler@4.6.0_@cloudflare+workers-types@4.20250402.0/node_modules/wrangler/bin/wrangler.js" "$@"
fi
