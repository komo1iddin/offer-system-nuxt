#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/vite-node@3.1.1_jiti@2.4.2_terser@5.39.0_yaml@2.7.1/node_modules/vite-node/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/vite-node@3.1.1_jiti@2.4.2_terser@5.39.0_yaml@2.7.1/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/vite-node@3.1.1_jiti@2.4.2_terser@5.39.0_yaml@2.7.1/node_modules/vite-node/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/vite-node@3.1.1_jiti@2.4.2_terser@5.39.0_yaml@2.7.1/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite-node@3.1.1_jiti@2.4.2_terser@5.39.0_yaml@2.7.1/node_modules/vite-node/vite-node.mjs" "$@"
else
  exec node  "$basedir/../../../../../../vite-node@3.1.1_jiti@2.4.2_terser@5.39.0_yaml@2.7.1/node_modules/vite-node/vite-node.mjs" "$@"
fi
