#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules/@nuxt/devtools/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules/@nuxt/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules/@nuxt/devtools/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules/@nuxt/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules:/Users/komo1iddin/Desktop/StudyBridge-Latest/v9/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules/@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../@nuxt+devtools@2.3.2_vite@6.2.4_jiti@2.4.2_terser@5.39.0_yaml@2.7.1__vue@3.5.13/node_modules/@nuxt/devtools/cli.mjs" "$@"
fi
